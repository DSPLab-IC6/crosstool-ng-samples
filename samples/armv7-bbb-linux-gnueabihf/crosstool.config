CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_ARCH_CPU="cortex-a8"
CT_ARCH_FPU="vfpv3-d16"
CT_ARCH_FLOAT_HW=y
CT_ARCH_arm=y
CT_ARCH_SUFFIX="v7"
CT_MULTILIB=y
CT_TARGET_VENDOR="bbb"
CT_TARGET_ALIAS="arm-linux-gnueabihf"
CT_KERNEL_linux=y
CT_KERNEL_LINUX_CUSTOM=y
CT_KERNEL_LINUX_CUSTOM_LOCATION="/usr/local/src/kernel-tarballs/linux-4.4.23-ti-rt-r51.tar.gz"
CT_CC_BINUTILS_SHOW_LINARO=y
CT_BINUTILS_LINARO_V_2_25=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_CC_LANG_CXX=y
CT_DEBUG_dmalloc=y
CT_DEBUG_duma=y
CT_DEBUG_gdb=y
CT_GDB_CROSS_STATIC=y
CT_GDB_CROSS_EXTRA_CONFIG_ARRAY="--disable-tui"
# CT_GDB_GDBSERVER_STATIC is not set
CT_DEBUG_ltrace=y
CT_DEBUG_strace=y
